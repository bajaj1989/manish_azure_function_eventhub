{
  "version": "2.0",
  "extensions": {
    "eventHubs": {
        "maxEventBatchSize" : 100,
        "minEventBatchSize" : 50,
        "maxWaitTime" : "00:00:00.5",
        "initialOffsetOptions" : {
          "type" : "fromEnd",
          "enqueuedTimeUtc" : ""
      }
    }
},
"logging": {
  "applicationInsights": {
    "samplingSettings": {
      "isEnabled": true,
      "excludedTypes": "Request"
    }
  }
},
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  }
}
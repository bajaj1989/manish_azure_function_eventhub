{
  "version": "2.0",
  "extensions": {
    "eventHubs": {
        "maxEventBatchSize" : 1000,
        "minEventBatchSize" : 500,
        "maxWaitTime" : "00:00:00.5"
    }
},
"logging": {
  "applicationInsights": {
    "samplingSettings": {
      "isEnabled": true,
      "excludedTypes": "Request"
    }
  }
},
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  }
}